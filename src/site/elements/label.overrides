/*******************************
         Site Overrides
*******************************/

.ui.inverted.label {
  background: @highlight;
  border: 1px solid @muted-text;
  color: @white;
  box-shadow: @main-dark;
}

.ui.inverted.no-outline.label {
  background: @highlight;
  border: 1px solid transparent;
  color: @white;
  box-shadow: @main-dark;
}

/* Link */
a.ui.inverted.label:hover {
  text-decoration: none;
  background: @basicHoverBackground;
  color: @basicHoverColor;
  box-shadow: @basicHoverBorder;
  box-shadow: @basicHoverBoxShadow;
}


/*-------------------
        Basic Inverted
--------------------*/

.ui.basic.inverted.label {
  background: @main-dark;
  border: @main-dark;
  color: @text !important;
  box-shadow: @main-dark;
}
